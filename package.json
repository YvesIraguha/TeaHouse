{
  "name": "teahouse",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development nodemon --exec babel-node index.js",
    "lint": "./node_modules/.bin/eslint .",
    "test": "NODE_ENV=test npm run migration:undo && NODE_ENV=test npm run migrate && NODE_ENV=test npm run seed && NODE_ENV=test nyc mocha --require @babel/register tests/* --timeout 10000 --exit ",
    "precommit": "npm run lint && npm run test",
    "migration:undo": "sequelize db:migrate:undo:all",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 50,
    "statements": 50,
    "functions": 50,
    "branches": 50
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/register": "^7.7.0",
    "@babel/runtime": "^7.7.2",
    "babel-preset-airbnb": "^4.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.7",
    "debug": "~2.6.9",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "mocha": "^6.2.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "sequelize-cli": "^5.5.1"
  }
}
